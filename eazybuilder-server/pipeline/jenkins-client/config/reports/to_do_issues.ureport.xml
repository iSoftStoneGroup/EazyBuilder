	ri.id,
	ri.subject as '主题' ,
     rs.name '看板',
	CONCAT(ru.lastname,ru.firstname)  '指派人',
	ri.created_on '创建时间'
FROM
	redmine_issues ri,
	redmine_sprints rs,
	issue_statuses s,
	redmine_projects rp ,
	redmine_users ru

WHERE
	ri.sprint_id = rs.id
	AND rs.is_product_backlog = 0
	AND ri.status_id = s.id
	AND s.name = "待认领"
	AND NOW() > DATE_ADD(ri.created_on, INTERVAL 3 DAY)
	AND 
	rp.id=ri.project_id
	AND 
	rp.name=:teamName
	AND 
	ru.id=ri.assigned_to_id
order by ri.created_on desc]]></sql><field name="id"/><field name="主题"/><field name="看板"/><field name="指派人"/><field name="创建时间"/><parameter name="teamName" type="String" default-value="ipaas"/></dataset></datasource><paper type="A5" left-margin="90" right-margin="90"
